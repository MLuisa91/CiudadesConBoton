<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:id="@+id/textViewCiudadIncompleta"
        android:layout_width="157dp"
        android:layout_height="49dp"
        tools:layout_editor_absoluteX="200dp"
        tools:layout_editor_absoluteY="50dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buttonAleatorio"
        android:layout_width="130dp"
        android:layout_height="57dp"
        android:text="Aleatorio"
        tools:layout_editor_absoluteX="37dp"
        tools:layout_editor_absoluteY="42dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="202dp"
        android:layout_height="38dp"
        android:text="Introduce la ciudad"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="398dp"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/editTextCiudad"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="157dp"
        tools:layout_editor_absoluteY="459dp"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/buttonComprobar"
        android:layout_width="186dp"
        android:layout_height="73dp"
        android:text="Comprobar"
        tools:layout_editor_absoluteX="171dp"
        tools:layout_editor_absoluteY="549dp"
        tools:ignore="MissingConstraints" />
</androidx.constraintlayout.widget.ConstraintLayout>